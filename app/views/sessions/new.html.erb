<h1 align="center">Show Me the Gifs!</h1>
<h1>&nbsp;</h1>
<h4 align="center">Please log in or create a new account.</h4>

<div class="form">
  <div align="center" class="col-md-6 col-md-offset-3">
    <%= form_for :session, url: login_path do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
  </div>
<h1>&nbsp;</h1>
  <div align="center" class="new account">
    <div class="col-md-6 col-md-offset-3">
  No account yet? Get one! <%= link_to "Sign up", new_user_path %>
</div>
</div>

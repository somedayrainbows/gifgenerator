<h1>Please log in</h1>
<div class="form">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for :session, url: login_path do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
  </div>
  <p>No account yet? Get one! <%= link_to "Sign up", new_user_path %></p>
</div>

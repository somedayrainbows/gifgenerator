<h1 align="center" >All Gif Categories</h1>

<div align="center" class="col-md-6 col-md-offset-3">
<% @categories.each do |category| %>
  <%= link_to category.name, category_path(category) %>
  <%= link_to "(delete)", admin_category_path(category), method: :delete if current_user.admin? %> &nbsp; &#9829; &nbsp;
<% end %>
</div>
